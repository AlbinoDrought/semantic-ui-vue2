{
  "name": "semantic-ui-vue2-albinodrought",
  "version": "2.5.6",
  "description": "Semantic UI Integration for Vue 2 - AlbinoDrought fork",
  "main": "index.js",
  "hsnext:main": "index.es.js",
  "private": true,
  "scripts": {
    "build": "node build/build.js",
    "publish": "npm run build && cd dist && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2016",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albinodrought/semantic-ui-vue2.git"
  },
  "keywords": [
    "vue",
    "vue2",
    "semantic-ui"
  ],
  "author": "Almino Melo <almino.melo@gmail.com>",
  "contributors": [
    "AlbinoDrought <albinodrought@gmail.com> (https://github.com/albinodrought)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/albinodrought/semantic-ui-vue2/issues"
  },
  "homepage": "https://github.com/albinodrought/semantic-ui-vue2#readme",
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "del": "^3.0.0",
    "rollup": "^0.47.5",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-vue": "^2.4.1",
    "vue": "^2.4.2"
  }
